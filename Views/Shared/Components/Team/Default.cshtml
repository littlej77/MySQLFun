@{
}
@model IEnumerable<int>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-teamid="" class="btn btn-outline-secondary">Home</a>

    @foreach (int t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-teamid="@t"
           class="btn @(ViewBag.SelectedTeam == t ? "btn-primary" : "btn btn-outline-secondary")">@t</a>

    }
</div>   